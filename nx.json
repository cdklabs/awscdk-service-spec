{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "compile",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/lib",
        "{projectRoot}/tsconfig.tsbuildinfo",
        "{projectRoot}/coverage",
        "{projectRoot}/test-reports",
        "{projectRoot}/dist"
      ],
      "inputs": [
        "!{projectRoot}/test-reports/**"
      ]
    },
    "compile": {
      "dependsOn": [
        "^compile"
      ],
      "outputs": [
        "{projectRoot}/lib",
        "{projectRoot}/tsconfig.tsbuildinfo"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/test-reports"
      ],
      "inputs": [
        "!{projectRoot}/test-reports/**"
      ]
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
